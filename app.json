{
  "expo": {
    "name": "QuranAppExpo",
    "slug": "QuranAppExpo",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "backgroundColor": "#1a237e"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "splash": {
        "backgroundColor": "#1a237e"
      }
    },
    "android": {
      "package": "com.anonymous.QuranAppExpo",
      "splash": {
        "backgroundColor": "#1a237e"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-font",
      "expo-splash-screen"
    ]
  }
}
